:- import ground/1 from basics.

isa_int(oid(Obj,NS),cid(Class,NS1)):- 
	ground(Obj),ground(Class),!,
%	debug(isa_int(oid(Obj,NS),cid(Class,NS1))),
	Call =.. [Class,Obj],
	last_known(Call).
isa_int(oid(Obj,NS),cid(Class,NS)):- 
	ground(Obj),var(Class),!,
	definedClass(Call,Class,Obj),
	last_known(Call).

