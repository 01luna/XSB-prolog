/* This demo shows the use of the meta programming (Hilog features). */

o1[m1(a1) -> r1].
o1[m2(b1,b2) -> r2].
o1[m3 -> r3].
o1[m4(c1,c2,c3) -> r4].

o2[M->R] :-
	o1[M->R],
	M=_F(_A1,_A2).

:- flProlog writeln/1.

?- writeln('all objects, all method invocations:'), X[M -> R].

?- writeln('Methods of o2:'), o2[M -> R].

