example3_1 :-
	Atoms = [p,q,r,s],
	smInit(4,Atoms),
	smAddRule(Atoms, p, [r], [q]),
	smAddRule(Atoms, q, [], [p]),
	smAddRule(Atoms, r, [], [s]),
	smAddRule(Atoms, s, [], [p]),
	smCommitProgram,
	smodelsPrintProgram,
	repeat,
	(smCompute ->
	    write('New Solution: '),nl,
	    smReturnModel(Atoms,Model),
	    write(Model),nl,
	    fail
	;
	    smEnd).
	