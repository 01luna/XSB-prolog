example7 :-
	Atoms = [d1a,d1b,d1c,saaa,saab,d2a,d3a,d3b],
	smInit(8,Atoms),
	smAddRule(Atoms,d1a,[],[]),
	smAddRule(Atoms,d1b,[],[]),
	smAddRule(Atoms,d1c,[],[]),
	smAddRule(Atoms,saaa,[d1a,d2a,d3a],[d3a]),
	smAddRule(Atoms,saab,[d1a,d2a,d3b],[d3a]),
	smAddRule(Atoms,saaa,[d1a],[]),
	smCommitProgram,
	smodelsPrintProgram,
	repeat,
	(smCompute ->
	    write('New Solution: '),nl,
	    smReturnModel(Atoms,Model),
	    write(Model),nl,
	    fail
	;
	    smEnd).
