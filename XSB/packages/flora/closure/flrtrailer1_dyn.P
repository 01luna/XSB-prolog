	'_$_$_flora_fd_dyn'(O,M,R),
	(M=Method ; '_$_$_flora_metheql_dyn'(M,Method)).

/****************************************************************************/
'_$_$_flora_mvd_rhs_dyn'(Object,Method,Result) :-
