/*** closure/flrtrailer1_dyn.P: appended automatically by the FLORA compiler
     to DYNAMIC code when equality maintenance is ENABLED 
 ***/


/*****************************************************************************
  closure rules for X::Y
*****************************************************************************/
:- table '_$_$_flora_subclass_dyn'/2.
:- table '_$_$_flora_strict_subclass_dyn'/2.

'_$_$_flora_subclass_dyn'(X,X) :- '_$_$_flora_exists_dyn'(X).
'_$_$_flora_subclass_dyn'(X,Y) :- '_$_$_flora_strict_subclass_dyn'(X,Y).

%% Using flora_subclass can be a major source of inefficiency
%% In most cases, you want to use '_$_$_flora_strict_subclass'
'_$_$_flora_subclass_dyn'(X,Y) :- '_$_$_flora_sub_dyn'(X,Y),  X \= Y.

'_$_$_flora_strict_subclass_dyn'(X,Y) :-
	'_$_$_flora_strict_subclass_dyn'(X,Z), 
	'_$_$_flora_strict_subclass_dyn'(Z,Y),
	%%(X \= Z -> true ; Z\=Y),
	( X = Y
	->  flora_warning_nl('cyclic subclass hierarchy, %S::%S::%S',
			     args(X,Z,Y))
	;   true
	).


/*****************************************************************************
  closure rules for X:Y
*****************************************************************************/
:- table '_$_$_flora_isa_dyn'/2.

'_$_$_flora_isa_dyn'(O,C) :-
	'_$_$_flora_strict_subclass_dyn'(C1,C),
	'_$_$_flora_isa_dyn'(O,C1).


/*****************************************************************************
  rules for monotonic inheritance of signatures
*****************************************************************************/
:- table '_$_$_flora_fs_dyn'/3.
:- table '_$_$_flora_mvs_dyn'/3.
:- table '_$_$_flora_ifs_dyn'/3.
:- table '_$_$_flora_imvs_dyn'/3.

%% Instance
'_$_$_flora_fs_dyn'(O,MethodArgs,R) :-
	'_$_$_flora_fs_dyn'(Class,MethodArgs,R),
	( '_$_$_flora_isa_dyn'(O,Class)
	  ;
	  '_$_$_flora_eql_dyn'(Class,C), '_$_$_flora_isa_dyn'(O,C)
	).
'_$_$_flora_ifs_dyn'(O,MethodArgs,R) :-
	'_$_$_flora_ifs_dyn'(Class,MethodArgs,R),
	( '_$_$_flora_isa_dyn'(O,Class)
	  ;
	  '_$_$_flora_eql_dyn'(Class,C), '_$_$_flora_isa_dyn'(O,C)
	).

'_$_$_flora_mvs_dyn'(O,MethodArgs,R) :-
	'_$_$_flora_mvs_dyn'(Class,MethodArgs,R),
	( '_$_$_flora_isa_dyn'(O,Class)
	  ;
	  '_$_$_flora_eql_dyn'(Class,C), '_$_$_flora_isa_dyn'(O,C)
	).
'_$_$_flora_imvs_dyn'(O,MethodArgs,R) :-
	'_$_$_flora_imvs_dyn'(Class,MethodArgs,R),
	( '_$_$_flora_isa_dyn'(O,Class)
	  ;
	  '_$_$_flora_eql_dyn'(Class,C), '_$_$_flora_isa_dyn'(O,C)
	).

%% Subclass
'_$_$_flora_fs_dyn'(Sub,MethodArgs,R) :-
	'_$_$_flora_fs_dyn'(Class,MethodArgs,R),
	( '_$_$_flora_strict_subclass_dyn'(Sub,Class)
	  ;
	  '_$_$_flora_eql_dyn'(Class,S), '_$_$_flora_strict_subclass_dyn'(Sub,S)
	).
'_$_$_flora_ifs_dyn'(Sub,MethodArgs,R) :-
	'_$_$_flora_ifs_dyn'(Class,MethodArgs,R),
	( '_$_$_flora_strict_subclass_dyn'(Sub,Class)
	  ;
	  '_$_$_flora_eql_dyn'(Class,S), '_$_$_flora_strict_subclass_dyn'(Sub,S)
	).

'_$_$_flora_mvs_dyn'(Sub,MethodArgs,R) :-
	'_$_$_flora_mvs_dyn'(Class,MethodArgs,R),
	( '_$_$_flora_strict_subclass_dyn'(Sub,Class)
	  ;
	  '_$_$_flora_eql_dyn'(Class,S), '_$_$_flora_strict_subclass_dyn'(Sub,S)
	).
'_$_$_flora_imvs_dyn'(Sub,MethodArgs,R) :-
	'_$_$_flora_imvs_dyn'(Class,MethodArgs,R),
	( '_$_$_flora_strict_subclass_dyn'(Sub,Class)
	  ;
	  '_$_$_flora_eql_dyn'(Class,S), '_$_$_flora_strict_subclass_dyn'(Sub,S)
	).


/*****************************************************************************
  rules for nonmonotonic behavior inheritance
*****************************************************************************/

:- table '_$_$_flora_fd_dyn'/3.
:- table '_$_$_flora_mvd_dyn'/3.
:- table '_$_$_flora_ifd_dyn'/3.
:- table '_$_$_flora_imvd_dyn'/3.
:- table '_$_$_flora_defined_fd_dyn'/2.
:- table '_$_$_flora_defined_mvd_dyn'/2.
:- table '_$_$_flora_defined_ifd_dyn'/2.
:- table '_$_$_flora_defined_imvd_dyn'/2.
:- table '_$_$_flora_overwritten_fd_dyn'/3.
:- table '_$_$_flora_overwritten_mvd_dyn'/3.
:- table '_$_$_flora_overwritten_ifd_dyn'/3.
:- table '_$_$_flora_overwritten_imvd_dyn'/3.
:- table '_$_$_flora_conflict_fd_dyn'/3.
:- table '_$_$_flora_conflict_ifd_dyn'/3.
:- table '_$_$_flora_conflict_mvd_dyn'/3.
:- table '_$_$_flora_conflict_imvd_dyn'/3.


'_$_$_flora_fd_dyn'(Object,Method,Value) :-
	%% If Object,Method are ground, check if Object[Method->...]
	%% is defined right away --an optimization
	(ground(Object), ground(Method) ->
	    tnot('_$_$_flora_defined_fd_dyn'(Object,Method)),
	    '_$_$_flora_isa_dyn'(Object,Class),
	    '_$_$_flora_ifd_dyn'(Class,Method,Value)
	  ;
	    '_$_$_flora_isa_dyn'(Object,Class),
	    '_$_$_flora_ifd_dyn'(Class,Method,Value),
	    ground(Object), ground(Method),
	    tnot('_$_$_flora_defined_fd_dyn'(Object,Method))
	),
	ground(Class),
	tnot('_$_$_flora_overwritten_fd_dyn'(Object,Class,Method)),
	ground(Class),
	tnot('_$_$_flora_conflict_fd_dyn'(Object,Class,Method)).

/* Should we add '_$_$_flora_eql' everywhere? Why not make '_$_$_flora_isa',
    '_$_$_flora_subclass', '_$_$_flora_ifd', etc. friendly to eql, so that 
   we won''t need to check for equality everywhere?
*/
'_$_$_flora_defined_fd_dyn'(Object,Method) :-
	'_$_$_flora_fd_dyn'(Object,Method,_).

'_$_$_flora_overwritten_fd_dyn'(Object,Class,Method) :-
	'_$_$_flora_isa_dyn'(Object,SubClass),
	'_$_$_flora_strict_subclass_dyn'(SubClass,Class),
	'_$_$_flora_ifd_dyn'(SubClass,Method,_).

%% When this is called, Object always belongs to Class
'_$_$_flora_conflict_fd_dyn'(Object,Class,Method) :-
	'_$_$_flora_defined_ifd_dyn'(Class1,Method),
	ground(Class1), ground(Class),
	Class \= Class1,
	'_$_$_flora_isa_dyn'(Object,Class1),
	tnot('_$_$_flora_strict_subclass_dyn'(Class1,Class)),
	tnot('_$_$_flora_strict_subclass_dyn'(Class,Class1)).


'_$_$_flora_mvd_dyn'(Object,Method,Value) :-
	(ground(Object), ground(Method) ->
	    tnot('_$_$_flora_defined_mvd_dyn'(Object,Method)),
	    '_$_$_flora_isa_dyn'(Object,Class),
	    '_$_$_flora_imvd_dyn'(Class,Method,Value)
	  ;
	    '_$_$_flora_isa_dyn'(Object,Class),
	    '_$_$_flora_imvd_dyn'(Class,Method,Value),
	    ground(Object), ground(Method),
	    tnot('_$_$_flora_defined_mvd_dyn'(Object,Method))
	),
	ground(Class),
	tnot('_$_$_flora_overwritten_mvd_dyn'(Object,Class,Method)),
	ground(Class),
	tnot('_$_$_flora_conflict_mvd_dyn'(Object,Class,Method)).

'_$_$_flora_defined_mvd_dyn'(Object,Method) :-
	'_$_$_flora_mvd_dyn'(Object,Method,_).

'_$_$_flora_overwritten_mvd_dyn'(Object,Class,Method) :-
	'_$_$_flora_isa_dyn'(Object,SubClass),
	'_$_$_flora_strict_subclass_dyn'(SubClass,Class),
	'_$_$_flora_imvd_dyn'(SubClass,Method,_).

'_$_$_flora_conflict_mvd_dyn'(Object,Class,Method) :-
	'_$_$_flora_defined_imvd_dyn'(Class1,Method),
	ground(Class1), ground(Class),
	Class \= Class1,
	'_$_$_flora_isa_dyn'(Object,Class1),
	tnot('_$_$_flora_strict_subclass_dyn'(Class1,Class)),
	tnot('_$_$_flora_strict_subclass_dyn'(Class,Class1)).


'_$_$_flora_ifd_dyn'(Class,Method,Value) :-
	(ground(Class), ground(Method) ->
	    tnot('_$_$_flora_defined_ifd_dyn'(Class,Method)),
	    '_$_$_flora_strict_subclass_dyn'(Class,Super),
	    '_$_$_flora_ifd_dyn'(Super,Method,Value)
	  ;
	    '_$_$_flora_strict_subclass_dyn'(Class,Super),
	    '_$_$_flora_ifd_dyn'(Super,Method,Value),
	    ground(Class), ground(Method),
	    tnot('_$_$_flora_defined_ifd_dyn'(Class,Method))
	),
	ground(Super),
	tnot('_$_$_flora_overwritten_ifd_dyn'(Class,Super,Method)),
	ground(Super),
	tnot('_$_$_flora_conflict_ifd_dyn'(Class,Super,Method)).

'_$_$_flora_defined_ifd_dyn'(Class,Method) :-
	'_$_$_flora_ifd_dyn'(Class,Method,_).

'_$_$_flora_overwritten_ifd_dyn'(Class,Super,Method) :-
	'_$_$_flora_strict_subclass_dyn'(Class,S),
	'_$_$_flora_strict_subclass_dyn'(S,Super),
	'_$_$_flora_ifd_dyn'(S,Method,_).

%% Note: when this is called, Class is always a subclass of Super
'_$_$_flora_conflict_ifd_dyn'(Class,Super,Method) :-
	'_$_$_flora_defined_ifd_dyn'(Super1,Method),
	ground(Super1), ground(Super),
	Super1 \= Super,
	'_$_$_flora_strict_subclass_dyn'(Class,Super1),
	tnot('_$_$_flora_strict_subclass_dyn'(Super1,Super)),
	tnot('_$_$_flora_strict_subclass_dyn'(Super,Super1)).


'_$_$_flora_imvd_dyn'(Class,Method,Value) :-
	(ground(Class), ground(Method) ->
	    tnot('_$_$_flora_defined_imvd_dyn'(Class,Method)),
	    '_$_$_flora_strict_subclass_dyn'(Class,Super),
	    '_$_$_flora_imvd_dyn'(Super,Method,Value)
	  ;
	    '_$_$_flora_strict_subclass_dyn'(Class,Super),
	    '_$_$_flora_imvd_dyn'(Super,Method,Value),
	    ground(Class), ground(Method),
	    tnot('_$_$_flora_defined_imvd_dyn'(Class,Method))
	),
	ground(Super),
	tnot('_$_$_flora_overwritten_imvd_dyn'(Class,Super,Method)),
	ground(Super),
	tnot('_$_$_flora_conflict_ifd_dyn'(Class,Super,Method)).

'_$_$_flora_defined_imvd_dyn'(Class,Method) :-
	'_$_$_flora_imvd_dyn'(Class,Method,_).

'_$_$_flora_overwritten_imvd_dyn'(Class,Super,Method) :-
	'_$_$_flora_strict_subclass_dyn'(Class,S),
	'_$_$_flora_strict_subclass_dyn'(S,Super),
	'_$_$_flora_imvd_dyn'(S,Method,_).

%% Note: when this is called, Class is always a subclass of Super
'_$_$_flora_conflict_imvd_dyn'(Class,Super,Method) :-
	'_$_$_flora_defined_imvd_dyn'(Super1,Method),
	ground(Super1), ground(Super),
	Super1 \= Super,
	'_$_$_flora_strict_subclass_dyn'(Class,Super1),
	tnot('_$_$_flora_strict_subclass_dyn'(Super1,Super)),
	tnot('_$_$_flora_strict_subclass_dyn'(Super,Super1)).


/*****************************************************************************
  rules for congruence due to single-valued methods
*****************************************************************************/
:- table '_$_$_flora_eql_dyn'/2.
:- table '_$_$_flora_metheql_dyn'/2.

'_$_$_flora_eql_dyn'(X,Y) :-
	'_$_$_flora_fd_dyn'(O,M1,X),
	(M1=M2 ; '_$_$_flora_metheql_dyn'(M1,M2)),
	'_$_$_flora_fd_dyn'(O,M2,Y),
	not X=Y.

'_$_$_flora_eql_dyn'(X,Y) :-
	'_$_$_flora_eql_dyn'(O1,O2),
	'_$_$_flora_fd_dyn'(O1,M1,X),
	(M1=M2 ; '_$_$_flora_metheql_dyn'(M1,M2)),
	'_$_$_flora_fd_dyn'(O2,M2,Y),
	not X=Y.

'_$_$_flora_eql_dyn'(X,Y) :-
	'_$_$_flora_eql_dyn'(X,Z),
	'_$_$_flora_eql_dyn'(Z,Y),
	not X=Y.


'_$_$_flora_metheql_dyn'(X,Y) :-
	( nonvar(X) ->
	    functor(X,'_$_$_flora_@',N), N >= 1,
	    functor(Y,'_$_$_flora_@',N)

	  ; nonvar(Y) ->
	    functor(Y,'_$_$_flora_@',N), N >= 1,
	    functor(X,'_$_$_flora_@',N)
	),
	'_$_$_flora_argeql_dyn'(X,Y,N),
	not X=Y.


'_$_$_flora_argeql_dyn'(_,_,0).

'_$_$_flora_argeql_dyn'(X,Y,N) :-
	N >= 1,
	arg(N,X,Xn),
	arg(N,Y,Yn),
	(Xn=Yn ; '_$_$_flora_eql_dyn'(Xn,Yn)),
	M is N-1,
	'_$_$_flora_argeql_dyn'(X,Y,M).


/*****************************************************************************
  rules for object existence and empty result sets
*****************************************************************************/
:- table '_$_$_flora_exists_dyn'/1.
:- table '_$_$_flora_mvd_dyn'/2.
:- table '_$_$_flora_imvd_dyn'/2.

'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_fd_dyn'(X,_,_).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_fd_dyn'(_,_,X).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_mvd_dyn'(X,_,_).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_mvd_dyn'(_,_,X).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_ifd_dyn'(X,_,_).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_ifd_dyn'(_,_,X).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_imvd_dyn'(X,_,_).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_imvd_dyn'(_,_,X).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_isa_dyn'(X,_).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_isa_dyn'(_,X).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_sub_dyn'(X,_).
'_$_$_flora_exists_dyn'(X) :- '_$_$_flora_sub_dyn'(_,X).

'_$_$_flora_exists_dyn'(X) :-
	'_$_$_flora_fd_dyn'(_,M,_),
	M =.. [_|L],
	member(X,L).

'_$_$_flora_exists_dyn'(X) :-
	'_$_$_flora_mvd_dyn'(_,M,_),
	M =.. [_|L],
	member(X,L).

'_$_$_flora_exists_dyn'(X) :-
	'_$_$_flora_ifd_dyn'(_,M,_),
	M =.. [_|L],
	member(X,L).

'_$_$_flora_exists_dyn'(X) :-
	'_$_$_flora_imvd_dyn'(_,M,_),
	M =.. [_|L],
	member(X,L).


'_$_$_flora_mvd_dyn'(O,M) :- '_$_$_flora_mvd_dyn'(O,M,_).

'_$_$_flora_imvd_dyn'(O,M) :- '_$_$_flora_imvd_dyn'(O,M,_).


/*****************************************************************************
  rules for flattened path expressions on RHS
*****************************************************************************/
:- table '_$_$_flora_fd_rhs_dyn'/3.
:- table '_$_$_flora_mvd_rhs_dyn'/3.
:- table '_$_$_flora_ifd_rhs_dyn'/3.
:- table '_$_$_flora_imvd_rhs_dyn'/3.
:- table '_$_$_flora_mvd_rhs_dyn'/2.
:- table '_$_$_flora_imvd_rhs_dyn'/2.
:- table '_$_$_flora_isa_rhs_dyn'/2.
:- table '_$_$_flora_sub_rhs_dyn'/2.
:- table '_$_$_flora_fs_rhs_dyn'/3.
:- table '_$_$_flora_mvs_rhs_dyn'/3.


/****************************************************************************/
'_$_$_flora_fd_rhs_dyn'(Object,Method,Result) :-
	'_$_$_flora_fd_dyn'(Object,M,Result),
	(M=Method ; '_$_$_flora_metheql_dyn'(M,Method)).

'_$_$_flora_fd_rhs_dyn'(Object,Method,Result) :-
	'_$_$_flora_eql_dyn'(Object,O),
	'_$_$_flora_fd_dyn'(O,M,Result),
	(M=Method ; '_$_$_flora_metheql_dyn'(M,Method)).

'_$_$_flora_fd_rhs_dyn'(Object,Method,Result) :-
