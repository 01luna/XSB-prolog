	'_$_$_flora_eql_dyn'(Object,O),
	'_$_$_flora_mvd_dyn'(O,M,R),
	(M=Method ; '_$_$_flora_metheql_dyn'(M,Method)).

/****************************************************************************/
'_$_$_flora_ifd_rhs_dyn'(Object,Method,Result) :-
	'_$_$_flora_ifd_dyn'(Object,M,Result),
	(M=Method ; '_$_$_flora_metheql_dyn'(M,Method)).

'_$_$_flora_ifd_rhs_dyn'(Object,Method,Result) :-
