%% user-specified time. In this case, the page is newer and is loaded.

url_modtime1:-  writeln('I am here...'), 

libwww_fetch_url('http://www.sunysb.edu/',[timeout(2),if_modified_since(5,5,1998,
2,12,2)], _R,S),
writeln(libwww_fetch_url('http://www.sunysb.edu/',[timeout(2),if_modified_since(5,5,1998,2,12,2)], _R,S)).

