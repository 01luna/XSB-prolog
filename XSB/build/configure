    # Perl base revision comes out as 5.0, so we strip .0
    perl_baserev=`perl -MConfig -e 'print "$Config{baserev}\n"' | sed -e 's/\..*//'`
    perl_patchlevel=`perl -MConfig -e 'print $Config{patchlevel}'`

