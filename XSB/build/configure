ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS build build_cpu build_vendor build_os host host_cpu host_vendor host_os CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT CPP INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA EGREP has_gcc SET_MAKE perl_support regmatch_support wildmatch_support perl_archlib perl_cc_compiler perl_ccflags perl_libs java_support javac java_path JAVA_HEADER_PATH JAVA_PLATFORM_HEADER_PATH itkwish_path DYNMOD_SHAREDLIB_EXTENSION DYNMOD_LINKFLAGS CXX CXXFLAGS ac_ct_CXX XASPLIBNAME XASPDEFFILE smoCFLAGS smoLDFLAGS smoLIB with_smodels smodels_goal xsb_version canonical full_config_name topdir mt_mode CROSS_SIMULATOR LD arch_config_prefix arch_install_config_prefix arch_config_file config_tag arch_bindir loaderCFLAGS builtinsCFLAGS emuloopCFLAGS trutilsCFLAGS GPPCFLAGS GPPWNTFLAG MEMFLAG LINKFLAGS emumake_goal os_version os_type compile_mode xsb_major_version xsb_minor_version xsb_beta_version xsb_codename release_year release_month release_monthname release_day xsb_scheduling_strategy site_prefix xsb_release_date xsb_full_version_name engine_mode ODBCSRC ODBCOBJ INTERPROLOGSRC INTERPROLOGOBJ mno_cyg_inclib mno_cyg_lib DLLTYPE win_site_includes win_ld_site_libraries xsb_src_extension_sans_dot xsb_hdr_extension_sans_dot xsb_obj_extension_sans_dot xsb_asm_extension_sans_dot xsb_dcl_extension_sans_dot xsb_src_extension xsb_hdr_extension xsb_obj_extension xsb_asm_extension xsb_dcl_extension LIBOBJS LTLIBOBJS'
