st(r14,T,(X,Te,T1))  :- T  = Te@ ^(X\T1).

s(T,Sa,Sb,A,B):- sc1(T,Sa,Sb,A,B).
s(T,Sa,Sb,A,B):-                  % s14
        s(T1,Sa,Sb1,A,B),
delete(sub(Te,X,_),Sb1,Sb), 
%not_occurs_in(X,Sb),
                st(r14,T,(X,Te,T1)).

delete(X,[X|L],L).
delete(X,[Y|L0],[Y|L1]) :- delete(X,L0,L1).

