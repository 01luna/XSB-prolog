/* This demo shows various cases of monotonic inheritance */

    a[b => c; bs=>>e].
    a[b1 => c1; bs1 =>> e1].
    aa[b=>d; bs1=>>d2].

    aaa::a.
    aa:aaa.

    tst1 :- aa[@T=>X], @T=..TT, write(TT), write('  '), writeln(X), fail.
    tst1.

    tst2 :- aaa[@U=>Y], @U=..UUU, write(UUU), write('  '), writeln(Y), fail.
    tst2.

    tst3 :- aa[@SM=>>V], @SM=..SMM, write(SMM), write('  '), writeln(V), fail.
    tst3.

test :- tell(temp), tst1, tst2, tst3, told.
