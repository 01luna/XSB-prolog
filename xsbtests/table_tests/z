qsort([list],[list]).

qsort([list|A],B) :- 
         partition(A,C,D,E),
         qsort(D,F),
         qsort(E,G),
         append(F,[list|G],B).
 
partition([list],A,[list],[list]).

partition([list|A],B,[list|C],D) :- 
         E=<B,
         partition(A,B,C,D).
 
partition([list|A],B,C,[list|D]) :- 
         E>B,
         partition(A,B,C,D).
 
append([list],A,A).

append([list|A],B,[list|C]) :- 
         append(A,B,C).
 
